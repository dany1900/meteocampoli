{
  "name": "meteocampoli",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c development",
    "build": "ng build --configuration production",
    "build-zip:ssr": "ng build --configuration production && gzipper compress ./dist/meteocampoli/browser && ng run meteocampoli:server",
    "build-prod": "ng build --configuration production  && gzip-all ./dist/meteocampoli/browser",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "json-server": "json-server --watch db.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --color",
    "build:client-and-server-bundles": "ng build --configuration production && ng run meteocampoli:server:production --bundle-dependencies all",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "dev:ssr": "ng run meteocampoli:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run meteocampoli:server:production",
    "prerender": "ng run meteocampoli:prerender",
    "start-pwa": "ng build --configuration production && http-server -p 8080 -c-1 dist/meteocampoli/browser",
    "predeploy": "ng build --configuration production --base-href=/meteocampoli/browser/",
    "deploy": "npx gh-pages -d dist/meteocampoli/browser",
    "purgecss": "purgecss --css src/assets/css/font-awesome.css --content src/index.html --output  src/assets/css/ && purgecss --css src/assets/css/bootstrap.css --content src/index.html --output  src/assets/css/ && purgecss --css src/assets/css/slider-carousel.css --content src/index.html --output  src/assets/css/ && purgecss --css src/assets/css/media-query.css --content src/index.html --output  src/assets/css/ && purgecss --css src/assets/css/styles.css --content src/index.html --output  src/assets/css/",
    "deploy-opt": "ng build --configuration production --aot --optimization --build-optimizer --vendor-chunk --common-chunk --delete-output-path --extract-licenses  --source-map=false",
    "deploy-test": "ng build --configuration production --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer"
  },
  "private": true,
  "dependencies": {
    "-": "0.0.1",
    "@angular-devkit/build-angular": "^13.3.4",
    "@angular/animations": "^13.3.5",
    "@angular/cdk": "^13.3.5",
    "@angular/common": "^13.3.5",
    "@angular/compiler": "^13.3.5",
    "@angular/core": "^13.3.5",
    "@angular/forms": "^13.3.5",
    "@angular/http": "^7.2.16",
    "@angular/localize": "^13.3.5",
    "@angular/material": "^13.3.5",
    "@angular/platform-browser": "^13.3.5",
    "@angular/platform-browser-dynamic": "^13.3.5",
    "@angular/platform-server": "^13.3.5",
    "@angular/router": "^13.3.5",
    "@angular/service-worker": "^13.3.5",
    "@ng-bootstrap/ng-bootstrap": "^11.0.1",
    "@nguniversal/express-engine": "^13.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^9.0.0-next.9",
    "acorn": "^8.7.1",
    "angular-animate": "^1.8.3",
    "angular-aria": "^1.8.3",
    "angular-crumbs": "^3.0.1",
    "angular-gtag": "^1.0.4",
    "angular-material": "^1.2.5",
    "angular-messages": "^1.8.3",
    "async": "^3.2.3",
    "bootstrap": "^3.4.1",
    "core-js": "^3.22.4",
    "express": "^4.18.1",
    "font-awesome": "^4.7.0",
    "g": "^2.0.1",
    "gzipper": "^7.1.0",
    "jquery": "^3.6.0",
    "jw-angular-social-buttons": "^1.0.0",
    "latest-version": "^6.0.0",
    "ng2-adsense": "^10.1.0",
    "ngx-device-detector": "^3.0.0",
    "npm-check-updates": "^12.5.11",
    "peer": "^0.6.1",
    "popper.js": "^1.16.1",
    "rxjs": "^7.5.5",
    "rxjs-compat": "^6.6.7",
    "trim-newlines": "^4.0.2",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular/cli": "^13.3.4",
    "@angular/compiler-cli": "^13.3.5",
    "@angular/language-service": "^13.3.5",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@nguniversal/builders": "^13.1.0",
    "@phenomnomnominal/angular-lazy-routes-fix": "^0.1.5",
    "@types/express": "^4.17.13",
    "@types/google.analytics": "0.0.42",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.31",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ts-loader": "^9.3.0",
    "ts-node": "~10.7.0",
    "tslint": "^6.1.3",
    "typescript": "~4.6.4",
    "webpack-cli": "^4.9.2"
  }
}
