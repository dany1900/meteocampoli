{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.*.css",
          "/assets/*.css",
          "/assets/*.CSV",
          "/assets/css/*.css",
          "/*.js",
          "/*.*.js",
          "/js/*.js",
          "/assets/js/*.js",
          "/*.png",
          "/*.jpg",
          "/*.php",
          "/pics/*.png",
          "/pics/*.gif",
          "/assets/*.png",
          "/assets/*.jpg",
          "/assets/img/*.jpg",
          "/assets/img/*.png",
          "/assets/storico-prato/*.CSV",
          "/assets/storico-prato/2024/*.CSV",
          "/assets/storico-prato/2025/*.CSV",
          "/assets/img/*.gif",
          "/assets/icons/*.png",
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani|CSV|csv)"
        ]
      }
    }
  ]
}
